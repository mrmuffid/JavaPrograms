package Day05;

public class Excer {

	public static void main(String[] args) {
		StringBuilder srk = new StringBuilder();
		int x = 121;
		int xNew = x;
		while (x>0)
		{
			int ur= x%10;
			System.out.print(ur);
			x/=10;
			srk.append(ur);
		}
		
		int number = Integer.parseInt(srk.toString());
		String yu =(number==xNew)?"Print True":"Print False";
		System.out.println();
		System.out.println(yu);
		
	
		
		

	}  

}

		

