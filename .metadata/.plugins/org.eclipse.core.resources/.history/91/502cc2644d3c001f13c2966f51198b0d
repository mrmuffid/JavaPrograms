package Day05;

public class Excer {

	public static void main(String[] args) {
		
		StringBuilder results = new StringBuilder();
		int x = 58965;
		int xn = x;
		int y=0;
		
		while (x>0)
		{
			y = x%10;
			System.out.print(y);
			results.append(y);
			x=x/10;

		}
		System.out.println();
		System.out.println("r");
		System.out.println(results);
		
		int nuber = Integer.parseInt(results.toString());
		
		String hola = (x==nuber) ? "true":"false";
		System.out.println(hola);

	}

}
